{
    "target_overrides": {
        "*": {
            "platform.stdio-baud-rate": 115200,
            "platform.stdio-buffered-serial": 1,
            "target.printf_lib": "std"
        },
        "MicroDAQHub": {
            "target.clock_source": "USE_PLL_HSE_EXTC",
            "target.stdio_uart_tx": "PB_6",
            "target.stdio_uart_rx": "PB_7",
            "target.hse_value": 25000000
        }
    }
}
